{
    "name": "brianrlewis/magic-admin",
    "description": "Unofficial implementation of Magic Admin SDK specification",
    "type": "library",
    "keywords": ["magic", "link", "fortmatic", "admin", "api", "sdk", "passwordless", "authentication"],
    "require": {
        "php": ">=7.1.0",
        "simplito/elliptic-php": "^1.0",
        "kornrunner/keccak": "^1.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0"
    },
    "autoload": {
        "psr-4": {
            "BrianRLewis\\MagicAdmin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianRLewis\\MagicAdmin\\Tests\\": "tests"
        }
    },
    "homepage":"https://github.com/brianrlewis/php-magic-admin",
    "license": "MIT",
    "scripts": {
        "php-fix": [
            "php-cs-fixer fix"
        ],
        "test": "phpunit"
    },
    "authors": [
        {
            "name": "Brian Lewis",
            "email": "brlewis23@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9",
        "friendsofphp/php-cs-fixer": "^2.16"
    }
}
